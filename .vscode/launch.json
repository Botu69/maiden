{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "maiden catalog list",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["--debug", "--config", "./maiden-dev.yaml", "catalog", "list"]
        },
        {
            "name": "Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceRoot}/app/src"
        }
    ]
}
